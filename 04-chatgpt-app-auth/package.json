{
  "name": "04-chatgpt-app-auth",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:auth": "tsx watch auth-server/server.ts",
    "dev:all": "concurrently -n NEXT,AUTH -c cyan,magenta \"npm run dev\" \"npm run dev:auth\"",

    "build": "next build --turbopack",
    "start": "next start",
    "start:auth": "tsx auth-server/server.ts",

    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "rebuild": "npm run clean && npm run build",
    "clean": "rm -rf .next"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.24.3",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mcp-handler": "1.0.4",
    "next": "16.0.10",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "4.1.13",
    "jsdom": "27.3.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/express": "^4.17.23",
    "@types/jsdom": "27.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "concurrently": "^9.2.1",
    "eslint": "9.39.1",
    "eslint-config-next": "16.0.10",
    "jsdom": "27.3.0",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.18",
    "tsx": "^4.20.3",
    "typescript": "5.9.3"
  }
}
